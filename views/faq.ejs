<!DOCTYPE html>
<html lang="en">

<head>
    <title>FAQ | MACMUN</title>
    <%- include('includes/head.ejs') %> 
    <link rel="stylesheet" href="/public/css/faq.css" />
    <link rel="shortcut icon" href="/public/images/logo240x240.png" type="image/x-icon" />
</head>

<body>
    <% if(lang == 'deutsch'){ %>
	<%- include('includes/de-navbars'); %>
	<% }else if(lang == 'turkce'){ %>
	<%- include('includes/tr-navbars'); %>
	<% }else{ %>
	<%- include('includes/navbars'); %>
	<% } %>
    <img class="home-logo" src="/public/images/logo240x240.png" onclick="window.location.href='/'" />
    <section>
        <div class="parent-container">
            <ul class="faq">
                 
                <% for(var i=0; i<data.length; i++) { %>
                <li>
                    <div class="question">
                        <h2>
                            <%-data[i].q%>
                            <div class="plus-minus-toggle collapsed"></div>
                        </h2>
                    </div>
                    <div class="answer">
                        <%- data[i].a %> 
                    </div>
                </li>
                <% } %> 
            </ul>
        </div>
    </section>
    <div onclick="window.open('https://dev-emre.web.app/', '_blank')" class="producer">
        Produced by Emre Ekiz
    </div>
    <div class="flags">
		<a href="/translate?lang=english&url=/conference/faq" class="en lang">
			<img src="/public/images/gb.webp" alt="English">
		</a>
		<a href="/translate?lang=turkce&url=/conference/faq" class="tr lang">
			<img src="/public/images/tr.webp" alt="Türkçe">
		</a>
		<a href="/translate?lang=deutsch&url=/conference/faq" class="de lang">
			<img src="/public/images/de.webp" alt="Deutsch">
		</a>
	</div>
    <style>.lang:first-child,.lang:last-child{-moz-border-radius-topleft:6px}.flags{-moz-border-radius-topleft:5px;-moz-border-radius-topleft:5px;position:fixed;display:flex;align-items:center;justify-content:center;flex-direction:column;-webkit-border-bottom-left-radius:5px;-webkit-border-top-left-radius:5px;border-top-left-radius:5px;border-bottom-left-radius:5px;background-color:transparent;right:0;bottom:25px;z-index:1000;border:1px solid #fff;border-right:none}.lang{cursor:pointer;padding:10px 15px 5px;transition:.3s linear .1s}.lang:hover{background:#fff}.lang:first-child{-webkit-border-top-left-radius:5px;border-top-left-radius:5px;border-bottom:1px solid #fff}.lang:last-child{-webkit-border-bottom-left-radius:5px;border-bottom-left-radius:5px;border-top:1px solid #fff}.lang img{width:30px}@media only screen and (max-width:900px){.lang img{width:20px}.lang{bottom:15px}}</style>
    <script src="/public/js/includes/navbars.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.8.0/gsap.min.js"
        integrity="sha512-eP6ippJojIKXKO8EPLtsUMS+/sAGHGo1UN/38swqZa1ypfcD4I0V/ac5G3VzaHfDaklFmQLEs51lhkkVaqg60Q=="
        crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.8.0/ScrollTrigger.min.js"
        integrity="sha512-CPA5LMoJI/a5HkSIAKcBtFXe4gqGjPUL2ExF/3PmhrrHI17wod9xOqqF+0WZQRKIIq0KwF8oG5BaiWobtrke3A=="
        crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="/public/js/faq.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.0/jquery.min.js"></script>
    <script>
        $(document).ready(function () {
            $("html").on("contextmenu", function (e) {
                return false;
            });
        });

    </script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
    <script>
        $(".faq li .question").click(function () {
            $(this).find(".plus-minus-toggle").toggleClass("collapsed"),
                $(this).parent().toggleClass("active");
        });

    </script>
</body>

</html>
